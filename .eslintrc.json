{
  "extends": ["airbnb", "prettier"],
  "plugins": [],
  "rules":{
    "react/prop-types": 0,
    "max-classes-per-file": [
      "error",
      { "ignoreExpressions": true, "max": 2 }
  ]
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}